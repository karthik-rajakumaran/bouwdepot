<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>BWD_calculateTotalAmount</name>
        <uuid>_a-0000dfc1-6117-8000-06a9-01ef9001ef90_4529</uuid>
        <description>This expression rule calculate the total amount</description>
        <parentUuid>_a-0000dfc1-6117-8000-06a9-01ef9001ef90_4525</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>euro(
  sum(
    if( isnull(ri!amountOne), 0.00, ri!amountOne),
    if( isnull(ri!amountTwo), 0.00, ri!amountTwo)
  )
)</definition>
        <namedTypedValue>
            <name>amountOne</name>
            <type>
                <name>double</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>amountTwo</name>
            <type>
                <name>double</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
